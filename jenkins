pipeline {
    agent any
    
    stages {
        stage ('gitcheckout') {
            steps {
                git branch: 'Project', url: 'https://github.com/psgola/lms-public.git'
            }
        }
        stage ('unit testing') {
        
            steps {
                Sh 'mvn test'
        }
        }
        }
        }
