pipeline {
    agent any
    
    stages {
        stage ('gitcheckout') {
            steps {
                git branch: 'Project', url: 'https://github.com/psgola/lms-public.git'
            }
        }
        stage ('build') {
             steps {
                   echo 'building..'
                   sh 'cd webapp && npm install'
         }
        }
        stage ('test') {
         steps {
                echo 'testing'
                sh 'cd webapp && npm test'
        }
        }
        }
        }
